      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },

"browser_action": {
      "default_icon": "/img/icon.png",
      "default_popup": "hello.html"
    },

    "permissions": [
      "http://ec2-15-165-118-201.ap-northeast-2.compute.amazonaws.com/", "tabs", "gcm", "notifications", "background"
    ]
